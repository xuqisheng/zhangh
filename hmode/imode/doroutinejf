for i in `cat /root/servers.txt | grep -v "^#" | grep -E "^${1:-.*}$"`; do

   echo
   echo " ========================================= server $i ======================================== "
   echo

   ssh $i "/root/mode/seecfg \"select * from cti_log\" portal"
done

