##
. /root/imode/cfg/.imode_aliases
#
for i in `cat /root/servers.txt | grep -E "^#[a-z]" | grep -E "^#${1:-.*}$" | sed -re "s/^#(.*)/\1/"`; do

   echo
   echo " ========================================= server $i ======================================== "
   echo
   ssh $i "echo ok"


done
exit

