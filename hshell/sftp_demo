#!/bin/bash
# ------------------------------------------------------------------
# function:
#          sftp 传输示例
#
# ------------------------------------------------------------------

# sftp 传输协议依赖ssh，故可使用ssh免密码登录方式进行数据传输

sftp -oPort=3305 root@192.168.0.28 << EOF
put /root/zhangh/inst_mode.py /home/testdb/
bye
EOF


